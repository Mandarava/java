<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration   
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="60000" />
		<setting name="callSettersOnNulls" value="true" />
		<setting name="cacheEnabled" value="true" />
	</settings>

	<typeAliases>

		<typeAlias type="com.mbp.qa.model.dto.MAI0050ArgValue" alias="MAI0050ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.MAI0061ArgValue" alias="MAI0061ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.QAMyNoticeDTO" alias="QAMyNoticeDTO" />
		<typeAlias type="com.mbp.qa.model.dto.QAMyWorkDTO" alias="QAMyWorkDTO" />
		<typeAlias type="com.mbp.qa.model.dto.QA0010ArgValue" alias="QA0010ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.QA0020ArgValue" alias="QA0020ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.QADetailDTO" alias="QADetailDTO" />
		<typeAlias type="com.mbp.qa.model.dto.QA0030ArgValue" alias="QA0030ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.QAHistoryDTO" alias="QAHistoryDTO" />
		<typeAlias type="com.mbp.qa.model.dto.QA0050ArgValue" alias="QA0050ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.CustomQADTO" alias="CustomQADTO" />
		<typeAlias type="com.mbp.qa.model.dto.QA0060ArgValue" alias="QA0060ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.BaseQADTO" alias="BaseQADTO" />
		<typeAlias type="com.mbp.qa.model.dto.QA0070ArgValue" alias="QA0070ArgValue" />
		<typeAlias type="com.mbp.qa.model.dto.ExcelQADTO" alias="ExcelQADTO" />
		<typeAlias type="com.mbp.qa.model.dto.ExcelProjectDTO" alias="ExcelProjectDTO" />
		<typeAlias type="com.mbp.qa.model.po.QA" alias="qa" />
		<typeAlias type="com.mbp.qa.model.dto.USE0031RetValue" alias="USE0031RetValue" />
		<typeAlias type="com.mbp.qa.model.dto.PRO0030RetValue" alias="PRO0030RetValue" />
		<typeAlias type="com.mbp.qa.model.dto.PRO0010RetValue" alias="PRO0010RetValue" />
		<typeAlias type="com.mbp.qa.model.dto.CST0010ArgValue" alias="CST0010ArgValue" />
		<typeAlias type="com.mbp.qa.model.po.CustomerMaster" alias="CustomerMaster" />
		<typeAlias type="com.mbp.qa.model.po.UserMaster" alias="UserMaster" />
		<typeAlias type="com.mbp.qa.model.po.DepartmentMaster" alias="DepartmentMaster" />
		<typeAlias type="com.mbp.qa.model.po.ClassMaster" alias="ClassMaster" />
		<typeAlias type="com.mbp.qa.model.po.Bug" alias="Bug" />


	</typeAliases>

	<plugins>
		<plugin interceptor="com.mbp.qa.interceptor.PageInterceptor">
			<property name="pageInterceptor" value="PageInterceptor" />
		</plugin>
	</plugins>

</configuration>